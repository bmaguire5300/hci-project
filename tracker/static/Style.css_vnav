

.nav-links{
	list-style-type: none;
	margin: 0;
	padding: 0;
	width: 200px;
	background-color: #333;
}
.nav-links a{
	display: block;
	color: #FFFFFF;
	padding: 8px 16px;
	text-decoration: none;
	font-size: 17px;
}
.nav-links a:hover{
	background-color: #93be34;
	color: white;
}

.nav-links a:active {
	background-color: #0000FF;
	color: white;
}

.nav-links li{
    list-style: none;
}

.card {
	margin: 0 auto;
	float: none
}

form { 
	width: 50%;
	margin-left: 25%
}